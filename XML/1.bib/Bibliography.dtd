<?xml version="1.0" encoding="UTF-8"?>
<!ELEMENT bib  ( book+ ) >
<!ELEMENT book  ( title , author* ,editor*,  publisher , price )>
<!ELEMENT author  ( last , first )>
<!ELEMENT editor  ( last , first, affiliation )>
